#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <time.h>
#include "utn_get.h"
#include "funcionesVivienda.h"

int iniVivienda(vivienda lista, int len)
{
	int retorno = -1;
	if (lista != NULL && len > 0) {
		retorno=0;
		for (int i = 0; i < len; i++) {
			*(lista+i)->legajoCensista = 0;//Buscamos todos los subindices del array y cambiamos el campo isEmpty a 1 para indicar que esta vacio
		}
	}

	return retorno;
}

